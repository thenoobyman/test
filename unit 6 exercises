import turtle
t = turtle.Turtle()
turtle.bgcolor ("black")
t.fillcolor("red")
t.shape("turtle")
t.color("red")
t.shape("circle")
t.speed(3)
t.width(5)
t.penup()
t.goto(200,-200)
t.begin_fill()
t. pendown()
t.forward(200)
t.left(90)
t.forward(200)
t.left(90)
t.forward(200)
t.left(90)
t.forward(200)
t.end_fill
t.left(90)
t.forward(200)
t.left(90)
t.forward(200)
for i in range(50):
    t.forward(8)
    t.left(1) 

t.done()
